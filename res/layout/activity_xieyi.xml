<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/wite"
    android:fitsSystemWindows="true"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/y88"
        android:background="@color/dialog_title_color"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/home_title"
            style="@style/home_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="用户协议" />

        <ImageView
            android:id="@+id/btn_xieyi_back"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_gravity="center_vertical"
            android:paddingBottom="@dimen/y10"
            android:paddingLeft="@dimen/x30"
            android:paddingRight="@dimen/x50"
            android:paddingTop="@dimen/y10"
            android:src="@drawable/back_botton" />
    </RelativeLayout>

    <WebView
        android:id="@+id/wb_xieyi"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</LinearLayout>